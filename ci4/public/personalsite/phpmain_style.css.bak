/* Animation */
@keyframes blinkTextCursor{
  from{border-right-color: rgba(255,255,255,.75);}
  to{border-right-color: transparent;}
}
@keyframes blinkTextCursor2{
  from{border-right-color: rgba(0,0,0,.75);}
  to{border-right-color: transparent;}
}
@keyframes typewriter{
  from{width: 0;}
  to{width: 7.55em;}
}
@keyframes typewriter2{
  from{width: 0;}
  to{width: 9.35em;}
}
@keyframes typewriter3{
  from{width: 0;}
  to{width: 11.10em;}
}
@keyframes typewriter4{
  from{width: 0;}
  to{width: 7.55em;}
}
@keyframes typewriter5{
  from{width: 0;}
  to{width: 6.35em;}
}
@keyframes fadeOutBG{
  from{opacity: 1;}
  to{opacity: 0;}
}

@keyframes removeContent{
  from{display: block;}
  to{display: none;}
}
@keyframes fadeInBG{
  from{opacity: 0;}
  to{opacity: 1;}
}
@keyframes addContent{
  from{display: none;}
  to{display: block;}
}
@keyframes fadeOutBG2{
  from{opacity: 1;}
  to{opacity: 0;}
}
@keyframes removeContent2{
  from{display: block;}
  to{display: none;}
}
@keyframes fadeInBG2{
  from{opacity: 0;}
  to{opacity: 1;}
}
@keyframes addContent2{
  from{display: none;}
  to{display: block;}
}