/* Pre-Functions */
@import url('https://fonts.googleapis.com/css2?family=Barlow:wght@100;400&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Anonymous+Pro&family=Barlow:wght@100;400&display=swap');

html{
	overflow: hidden;
	background-image: url("image/personal_background3.png");
	background-repeat: no-repeat;
	background-size: cover;
	color: white;
}
h2{
	font-family: 'Anonymous Pro', monospace;
	padding-left: 10px;
}
h3{
	font-family: 'Anonymous Pro', monospace;
	padding-left: 10px;
}
#main{
	font-family: 'Barlow', sans-serif;
	padding-left: 10px;
}
a{
	font-family: 'Barlow', sans-serif;
	color: gray;
	padding-left: 10px;
}
a:active{
	text-decoration: none;
}
a:hover{
	text-decoration: none;
}
a:link{
	text-decoration: none;
}'
a:visited{
	text-decoration: none;
}
