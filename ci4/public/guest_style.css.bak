/* Pre-Functions */
@import url('https://fonts.googleapis.com/css2?family=Barlow:wght@100;400&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Anonymous+Pro&family=Barlow:wght@100;400&display=swap');

html{
	overflow: hidden;
}
body{
	position: fixed;
	width: 1920px;
	height: 1080px;
	background-image: url('image/personal_background3.png');
	background-repeat: no-repeat;
	background-size: cover;
}
/* List */
#main_guest{
	position: relative;
	left: 2%;
	text-align: left;
	font-family: 'Anonymous Pro', monospace;
	color: white;
	font-size: 16px;
	opacity: 0;
	animation: fadeInBG2 1s linear 0s 1 normal forwards;
	animation-delay: 1s;
}
/* Animation */
@keyframes fadeInBG2{
  from{opacity: 0;}
  to{opacity: 1;}
}